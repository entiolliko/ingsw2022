   <VBox alignment="CENTER" layoutX="850.0" layoutY="500.0" prefHeight="150.0" prefWidth="220.0" stylesheets="@../css_files/EntryScreenCSS.css">
      <children>
         <Button fx:id="enterButton" mnemonicParsing="false" onAction="#enterButtonClick" prefHeight="75.0" prefWidth="220.0" text="Play" textFill="#3bfff8" />
